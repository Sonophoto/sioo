PARSER_BEGIN(Example)

public class Example {

  public static void main(String args[]) throws ParseException {
    Example parser = new Example(System.in);
    parser.funny_list();
  }

}

PARSER_END(Example)

SKIP :
{
  " "
| "\t"
| "\n"
| "\r"
}

void identifier_list() :
{}
{
  <ID> ( LOOKAHEAD(2) "," <ID> )*
}

void funny_list() :
{}
{
  identifier_list() "," <INT>
}

TOKEN [IGNORE_CASE] :
{
  <ID: (["a"-"z"])+>
|
  <INT: (["0"-"9"])+>
}
